<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<record id="view_ship_carrier_ups_form" model="ir.ui.view">
            <field name="name">view.ship.carrier.ups.form</field>
            <field name="model">ship.carrier</field>
      		<field name="inherit_id" ref="sale_store.view_ship_carrier_form" />
            <field name="arch" type="xml">
            	<xpath expr="//group" positon="inside">
            		<group string="UPS Credentials">
            			<field name="ups_access_license_number"/>
                        <field name="ups_user_id"/>
                        <field name="ups_password"/>
                        <field name="ups_shipper_number"/>
                        <field name="ups_environment"/>
            		</group>
            	</xpath>
            </field>
        </record>
    </data>
</odoo>
