<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_return_percent_wizard" model="ir.ui.view">
            <field name="name">view.return_percent.wizard</field>
            <field name="model">return.percent.wizard</field>
            <field name="arch" type="xml">
                <form string="Returns Percent">
                    <group>
                        <group>
                            <field name="from_date"/>
                            <field name="to_date"/>
                        </group>
                    </group>
                    <footer>
                      <button name="button_download_report" type="object" string="Download Report" class="oe_highlight"/>
                      or
                      <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_return_percent_wizard" model="ir.actions.act_window">
            <field name="name">Returns Percent</field>
            <field name="res_model">return.percent.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
