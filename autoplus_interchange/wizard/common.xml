<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
    <record id="custom_message_wizard_view_form" model="ir.ui.view">
        <field name="name">custom.message.form</field>
        <field name="model">custom.message</field>
        <field name="arch" type="xml">
            <form string="Report">
                <div>
                    <field name="text" nolabel="1"/>
                </div>
                <footer>
                   <button string="Close" class="btn-default" special="cancel"/>
                </footer>
           </form>
        </field>
    </record>
</data>
</odoo>
