<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_change_product_quantity_inh" model="ir.ui.view">
            <field name="name">Change Product Quantity Inh</field>
            <field name="model">stock.change.product.qty</field>
            <field name="inherit_id" ref="stock.view_change_product_quantity"/>
            <field name="arch" type="xml">
                <field name="new_quantity" position="after">
                    <field name="product_cost"/>
                </field>
            </field>
        </record>

</odoo>

