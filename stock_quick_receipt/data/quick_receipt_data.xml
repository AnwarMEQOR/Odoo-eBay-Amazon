<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_stock_quick_receipt" model="ir.sequence">
            <field name="name">Quick Receipt</field>
            <field name="code">stock.quick.receipt</field>
            <field name="prefix">WHQR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_stock_quick_transfer" model="ir.sequence">
            <field name="name">Quick Transfer</field>
            <field name="code">stock.quick.transfer</field>
            <field name="prefix">WHQT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
